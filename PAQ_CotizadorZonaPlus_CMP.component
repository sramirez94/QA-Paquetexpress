                                $scope.Wrapper.mapHierarchy = result.mapHierarchy;
                                j$.each(result.mapHierarchy,
                                        function(key, userId){
                                            /*if(key.toUpperCase().includes('GERENTE SUCURSAL'))
                                                $scope.Managers['GSU'] = userId;
